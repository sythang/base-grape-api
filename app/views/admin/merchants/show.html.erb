<%= render "admin/shared/flash_message" %>
<div class="card">
  <div class="card-header bg-light"></div>
  <div class="card-body">
    <dt>ID</dt>
    <dd><%= @merchant.id %></dd>
    <dt>Name</dt>
    <dd><%= @merchant.name %></dd>
    <dt>Description</dt>
    <dd><%= @merchant.description %></dd>
    <dt>Logo</dt>
    <dd><%= image_tag @merchant.logo.url, class: "img-responsive", width: 200 %></dd>
    <%= link_to 'Edit', edit_admin_merchant_path(@merchant) %> |
    <%= link_to 'Back', admin_merchants_path %>
  </div>
</div>
